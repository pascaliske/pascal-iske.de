{
  "hosting": {
    "public": "dist/app",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/cfns/send-contact-request",
        "function": "sendContactRequest"
      }
    ]
  },
  "functions": {
    "source": "dist/cfn"
  }
}
