{
  "name": "pascal-iske",
  "description": "Angular Web App built with Travis CI and deployed with Firebase.",
  "version": "1.0.14",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "firebase": "firebase",
    "start": "ng serve",
    "build": "ng build --prod --aot --build-optimizer",
    "build-cfns": "tsc --project src/tsconfig.cfn.json && cp src/cfn/package.json dist/cfn/package.json",
    "test": "ng test",
    "lint": "ng lint pascal-iske --fix",
    "e2e": "ng e2e",
    "check": "snyk test",
    "format": "prettier --config .prettierrc --write src/**/**.{ts,scss,md}",
    "docs": "compodoc --tsconfig src/tsconfig.app.json --output docs/ --name \"Pascal Iske\" --theme material --hideGenerator --port 4201"
  },
  "private": true,
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "dependencies": {
    "@angular/animations": "6.1.1",
    "@angular/common": "6.1.1",
    "@angular/compiler": "6.1.1",
    "@angular/core": "6.1.1",
    "@angular/forms": "6.1.1",
    "@angular/http": "6.1.1",
    "@angular/platform-browser": "6.1.1",
    "@angular/platform-browser-dynamic": "6.1.1",
    "@angular/platform-server": "6.1.1",
    "@angular/pwa": "^0.7.2",
    "@angular/router": "6.1.1",
    "@angular/service-worker": "6.1.1",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "@ngx-progressbar/core": "^5.0.1",
    "@ngx-progressbar/http": "^5.0.1",
    "@ngx-progressbar/router": "^5.0.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@pascaliske/html-helpers": "^1.1.0",
    "@pascaliske/ngx-notifications": "^1.0.7",
    "@sentry/browser": "^4.0.0-beta.12",
    "classlist.js": "^1.1.20150312",
    "cookieconsent": "^3.1.0",
    "core-decorators": "^0.20.0",
    "core-js": "^2.5.7",
    "cors": "^2.8.4",
