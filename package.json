{
  "name": "pascal-iske",
  "description": "Angular Web App built with Travis CI and deployed with Firebase.",
  "version": "1.4.10",
  "license": "MIT",
  "scripts": {
    "ng": "ngw",
    "firebase": "firebase",
    "bundlewatch": "bundlewatch --config .bundlewatchrc.json",
    "report:generate": "lighthouse --quiet --chrome-flags=\"--headless\" --output-path=./report.html https://$PROD_DOMAIN",
    "report:upload": "now report.html --token $NOW_TOKEN --no-clipboard --public",
    "start": "ngw serve",
    "start:local": "serve --config serve.json --listen 4201 --no-clipboard",
    "build": "yarn run build:app && yarn run build:server && yarn run build:cfn && yarn run build:prerender",
    "build:app": "ngw build --prod --stats-json",
    "build:server": "ngw run pascal-iske:server --configuration production",
    "build:cfn": "tsc --project src/tsconfig.cfn.json && cp src/cfn/package.json dist/cfn/package.json",
    "build:prerender": "ts-node --require esm src/prerender.ts",
    "lint": "ngw lint pascal-iske --fix",
    "check": "snyk test && snyk monitor",
    "format": "prettier --config .prettierrc --write \"src/**/*.{ts,scss,html,md}\"",
    "changelog": "standard-changelog"
  },
  "private": true,
  "greenkeeper": {
    "ignore": [
      "typescript",
      "webpack"
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.2.2",
    "@angular/common": "^7.2.2",
    "@angular/compiler": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/platform-server": "^7.2.2",
    "@angular/pwa": "^0.12.3",
    "@angular/router": "^7.2.2",
    "@angular/service-worker": "^7.2.2",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.13",
    "@fortawesome/free-brands-svg-icons": "^5.7.0",
    "@fortawesome/free-regular-svg-icons": "^5.7.0",
    "@fortawesome/free-solid-svg-icons": "^5.7.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "@ngx-progressbar/core": "~5.3.1",
    "@ngx-progressbar/http": "~5.3.1",
    "@ngx-progressbar/router": "~5.3.1",
    "@pascaliske/form-elements": "^1.2.2",
    "@pascaliske/html-helpers": "^1.2.2",
    "@pascaliske/ngx-notifications": "^1.1.0",
    "@pascaliske/ngx-sentry": "^1.3.1",
    "cookieconsent": "^3.1.0",
    "core-js": "^2.6.3",
    "highlight.js": "^9.13.1",
    "intersection-observer": "^0.5.1",
    "lodash-es": "^4.17.11",
    "modularscale-sass": "^3.0.8",
    "ngx-cookieconsent": "^2.0.0",
    "ngx-markdown": "^7.1.2",
    "normalize-scss": "^7.0.1",
    "rxjs": "^6.3.3",
    "susy": "^3.0.5",
    "tslib": "^1.9.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.3",
    "@angular/cli": "^7.2.3",
    "@angular/compiler-cli": "^7.2.2",
    "@angular/language-service": "^7.2.2",
    "@commitlint/cli": "^7.4.0",
    "@commitlint/config-conventional": "^7.3.1",
    "@pascaliske/ngx-prerenderer": "^1.0.1",
    "@types/cors": "^2.8.4",
    "@types/fs-extra": "^5.0.4",
    "@types/google.analytics": "^0.0.39",
    "@types/highlight.js": "^9.12.3",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "10.12.19",
    "@types/nodemailer": "^4.6.5",
    "@types/webpack": "^4.4.24",
    "bundlewatch": "^0.2.1",
    "chalk": "^2.4.2",
    "codelyzer": "^4.5.0",
    "content-replace-webpack-plugin": "^0.0.4",
    "cors": "^2.8.5",
    "esm": "^3.1.4",
    "firebase-admin": "^6.5.1",
    "firebase-functions": "^2.1.0",
    "firebase-tools": "^6.3.0",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "lighthouse": "^4.1.0",
    "lint-staged": "^8.1.1",
    "ngw": "^7.0.0",
    "nodemailer": "^5.1.1",
    "now": "^13.1.2",
    "prettier": "^1.16.1",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "rxjs-tslint-rules": "^4.16.1",
    "serve": "^10.1.1",
    "snyk": "^1.123.0",
    "standard-changelog": "^2.0.6",
    "ts-node": "~8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.2.4",
    "webpack": "4.29.0",
    "webpack-dashboard": "^2.1.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
