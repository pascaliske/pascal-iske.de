{
  "name": "pascal-iske",
  "description": "Angular Web App built with Travis CI and deployed with Firebase.",
  "version": "1.5.8",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "bump": "bump package.json --yes",
    "firebase": "firebase",
    "bundlewatch": "bundlewatch --config .bundlewatchrc.json",
    "start": "ng serve",
    "start:local": "serve --config serve.json --listen 4200 --no-clipboard",
    "build": "run-s build:app build:server build:cfn build:prerender",
    "build:app": "ng build --prod",
    "build:server": "ng run pascal-iske:server:production",
    "build:cfn": "tsc --project tsconfig.cfn.json && cp src/cfn/package.json dist/cfn/package.json",
    "build:prerender": "ts-node --require esm src/prerender.ts",
    "now-build": "yarn run build",
    "lint": "ng lint pascal-iske --fix && stylelint src/**/*.scss",
    "check": "snyk test; snyk monitor",
    "e2e": "cypress verify && start-server-and-test start:local http://localhost:4200 'cypress run --record false'",
    "format": "prettier --config .prettierrc --write \"src/**/*.{ts,scss,html,md}\"",
    "changelog": "standard-changelog"
  },
  "private": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "optionalDependencies": {
    "fsevents": "2.1.2"
  },
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/forms": "^9.0.7",
    "@angular/platform-browser": "^9.0.7",
    "@angular/platform-browser-dynamic": "^9.0.7",
    "@angular/platform-server": "^9.0.7",
    "@angular/router": "^9.0.7",
    "@angular/service-worker": "^9.0.7",
    "@fortawesome/angular-fontawesome": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@ngx-progressbar/core": "~5.3.2",
    "@ngx-progressbar/http": "~5.3.2",
    "@ngx-progressbar/router": "~5.3.2",
    "@pascaliske/form-elements": "^2.4.3",
    "@pascaliske/html-helpers": "^1.5.7",
    "@pascaliske/ngx-notifications": "^1.4.0",
    "@pascaliske/ngx-sentry": "^2.2.2",
    "@sentry/browser": "^5.15.0",
    "@types/cors": "^2.8.6",
    "cookieconsent": "^3.1.1",
    "core-js": "^3.6.4",
    "flatpickr": "^4.6.3",
    "fuzzysearch": "^1.0.3",
    "intersection-observer": "^0.7.0",
    "lodash-es": "^4.17.15",
    "modularscale-sass": "^3.0.10",
    "ngx-cookieconsent": "^2.2.3",
    "ngx-markdown": "^8.2.2",
    "normalize-scss": "^7.0.1",
    "rxjs": "^6.5.4",
    "susy": "^3.0.5",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.0.0",
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular/cli": "^9.0.7",
    "@angular/compiler-cli": "^9.0.7",
    "@angular/language-service": "^9.0.7",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@packtracker/webpack-plugin": "^2.2.0",
    "@pascaliske/ngx-prerenderer": "^2.0.3",
    "@sentry/cli": "^1.51.1",
    "@types/fs-extra": "^8.1.0",
    "@types/google.analytics": "^0.0.40",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "13.9.3",
    "@types/nodemailer": "^6.4.0",
    "@types/webpack": "^4.41.9",
    "bump-cli": "^1.1.3",
    "bundlewatch": "^0.2.6",
    "chalk": "^3.0.0",
    "codelyzer": "^5.1.2",
    "cors": "^2.8.5",
    "cypress": "^4.2.0",
    "esm": "^3.2.25",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.3.0",
    "firebase-tools": "^7.16.0",
    "fs-extra": "^8.1.0",
    "husky": "^4.2.3",
    "lighthouse": "^5.6.0",
    "lint-staged": "^10.0.9",
    "nodemailer": "^6.4.6",
    "now": "^17.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "rxjs-tslint-rules": "^4.29.2",
    "serve": "^11.3.0",
    "snyk": "^1.303.2",
    "standard-changelog": "^2.0.21",
    "start-server-and-test": "^1.10.11",
    "stylelint": "^13.2.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "ts-loader": "^6.2.2",
    "ts-node": "~8.8.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5",
    "webpack": "4.42.1",
    "webpack-dashboard": "^3.2.0",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
