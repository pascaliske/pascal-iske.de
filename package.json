{
  "name": "pascal-iske",
  "description": "Angular Web App built with Travis CI and deployed with Firebase.",
  "version": "1.4.18",
  "license": "MIT",
  "scripts": {
    "ng": "ngw",
    "firebase": "firebase",
    "bundlewatch": "bundlewatch --config .bundlewatchrc.json",
    "report:generate": "lighthouse --quiet --chrome-flags=\"--headless\" --output-path=./report.html https://$PROD_DOMAIN",
    "report:upload": "now report.html --token $NOW_TOKEN --no-clipboard --public",
    "sentry:commits": "sentry-cli releases new $TRAVIS_TAG && sentry-cli releases set-commits --auto $TRAVIS_TAG",
    "start": "ngw serve",
    "start:local": "serve --config serve.json --listen 4201 --no-clipboard",
    "build": "yarn run build:app && yarn run build:server && yarn run build:cfn && yarn run build:prerender",
    "build:app": "ngw build --prod --stats-json",
    "build:server": "ngw run pascal-iske:server --configuration production",
    "build:cfn": "tsc --project src/tsconfig.cfn.json && cp src/cfn/package.json dist/cfn/package.json",
    "build:prerender": "ts-node --require esm src/prerender.ts",
    "lint": "ngw lint pascal-iske --fix && stylelint src/**/*.scss",
    "check": "snyk test && snyk monitor",
    "format": "prettier --config .prettierrc --write \"src/**/*.{ts,scss,html,md}\"",
    "changelog": "standard-changelog"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/common": "^7.2.14",
    "@angular/compiler": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/forms": "^7.2.14",
    "@angular/http": "^7.2.14",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/platform-server": "^7.2.14",
    "@angular/pwa": "^0.13.8",
    "@angular/router": "^7.2.14",
    "@angular/service-worker": "^7.2.14",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@ngx-progressbar/core": "~5.3.2",
    "@ngx-progressbar/http": "~5.3.2",
    "@ngx-progressbar/router": "~5.3.2",
    "@pascaliske/form-elements": "^2.2.0",
    "@pascaliske/html-helpers": "^1.4.0",
    "@pascaliske/ngx-notifications": "^1.2.0",
    "@pascaliske/ngx-sentry": "^2.0.0",
    "@sentry/browser": "^5.1.1",
    "cookieconsent": "^3.1.0",
    "core-js": "^2.6.5",
    "flatpickr": "^4.5.7",
    "fuzzysearch": "^1.0.3",
    "intersection-observer": "^0.7.0",
    "lodash-es": "^4.17.11",
    "modularscale-sass": "^3.0.8",
    "ngx-cookieconsent": "^2.2.0",
    "ngx-markdown": "^7.1.5",
    "normalize-scss": "^7.0.1",
    "rxjs": "^6.3.3",
    "susy": "^3.0.5",
    "tslib": "^1.9.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "^7.2.14",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@pascaliske/ngx-prerenderer": "^1.1.0",
    "@sentry/cli": "^1.41.1",
    "@sentry/webpack-plugin": "^1.6.2",
    "@types/cors": "^2.8.4",
    "@types/fs-extra": "^7.0.0",
    "@types/google.analytics": "^0.0.39",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "10.12.18",
    "@types/nodemailer": "^6.1.0",
    "@types/webpack": "^4.4.27",
    "bundlewatch": "^0.2.1",
    "chalk": "^2.4.2",
    "codelyzer": "^5.0.0",
    "content-replace-webpack-plugin": "^0.0.4",
    "cors": "^2.8.5",
    "esm": "^3.2.22",
    "firebase-admin": "^7.3.0",
    "firebase-functions": "^2.3.0",
    "firebase-tools": "^6.7.1",
    "fs-extra": "^8.0.0",
    "husky": "^2.0.0",
    "lighthouse": "^5.0.0",
    "lint-staged": "^8.1.5",
    "ngw": "^7.0.0",
    "nodemailer": "^6.1.1",
    "now": "^15.0.6",
    "prettier": "^1.17.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "rxjs-tslint-rules": "^4.23.0",
    "serve": "^11.0.0",
    "snyk": "^1.153.0",
    "standard-changelog": "^2.0.7",
    "stylelint": "^10.0.1",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "ts-node": "~8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.4",
    "webpack": "4.32.0",
    "webpack-dashboard": "^3.0.4",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
