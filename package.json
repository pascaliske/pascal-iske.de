{
  "name": "pascal-iske",
  "description": "Angular Web App built with Travis CI and deployed with Firebase.",
  "version": "1.4.18",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "firebase": "firebase",
    "bundlewatch": "bundlewatch --config .bundlewatchrc.json",
    "report:generate": "lighthouse --quiet --chrome-flags=\"--headless\" --output-path=./report.html https://$PROD_DOMAIN",
    "report:upload": "now report.html --token $NOW_TOKEN --no-clipboard --public",
    "sentry:commits": "sentry-cli releases new $TRAVIS_TAG && sentry-cli releases set-commits --auto $TRAVIS_TAG",
    "start": "ng serve",
    "start:local": "serve --config serve.json --listen 4200 --no-clipboard",
    "build": "yarn run build:app && yarn run build:server && yarn run build:cfn && yarn run build:prerender",
    "build:app": "ng build --prod --stats-json",
    "build:server": "ng run pascal-iske:server:production",
    "build:cfn": "tsc --project tsconfig.cfn.json && cp src/cfn/package.json dist/cfn/package.json",
    "build:prerender": "ts-node --require esm src/prerender.ts",
    "lint": "ng lint pascal-iske --fix && stylelint src/**/*.scss",
    "check": "snyk test && snyk monitor",
    "e2e": "cypress verify && start-server-and-test start:local http://localhost:4200 'cypress run --record'",
    "format": "prettier --config .prettierrc --write \"src/**/*.{ts,scss,html,md}\"",
    "changelog": "standard-changelog"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/platform-server": "^8.0.0",
    "@angular/pwa": "^0.800.2",
    "@angular/router": "^8.0.0",
    "@angular/service-worker": "^8.0.0",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/free-regular-svg-icons": "^5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@ngx-progressbar/core": "~5.3.2",
    "@ngx-progressbar/http": "~5.3.2",
    "@ngx-progressbar/router": "~5.3.2",
    "@pascaliske/form-elements": "^2.2.1",
    "@pascaliske/html-helpers": "^1.5.2",
    "@pascaliske/ngx-notifications": "^1.3.0",
    "@pascaliske/ngx-sentry": "^2.1.0",
    "@sentry/browser": "^5.4.0",
    "@types/cors": "^2.8.5",
    "cookieconsent": "^3.1.1",
    "core-js": "^2.6.5",
    "flatpickr": "^4.6.1",
    "fuzzysearch": "^1.0.3",
    "intersection-observer": "^0.7.0",
    "lodash-es": "^4.17.11",
    "modularscale-sass": "^3.0.8",
    "ngx-cookieconsent": "^2.2.2",
    "ngx-markdown": "^8.0.2",
    "normalize-scss": "^7.0.1",
    "rxjs": "^6.5.2",
    "susy": "^3.0.5",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.0.2",
    "@angular-devkit/build-angular": "^0.800.2",
    "@angular/cli": "^8.0.2",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@pascaliske/ngx-prerenderer": "^2.0.0",
    "@sentry/cli": "^1.44.3",
    "@sentry/webpack-plugin": "^1.6.2",
    "@types/fs-extra": "^7.0.0",
    "@types/google.analytics": "^0.0.40",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "12.0.7",
    "@types/nodemailer": "^6.2.0",
    "@types/webpack": "^4.4.32",
    "bundlewatch": "^0.2.1",
    "chalk": "^2.4.2",
    "codelyzer": "^5.1.0",
    "cors": "^2.8.5",
    "cypress": "^3.3.1",
    "esm": "^3.2.25",
    "firebase-admin": "^8.1.0",
    "firebase-functions": "^2.3.1",
    "firebase-tools": "^6.11.0",
    "fs-extra": "^8.0.1",
    "husky": "^2.4.0",
    "lighthouse": "^5.1.0",
    "lint-staged": "^8.2.0",
    "nodemailer": "^6.2.1",
    "now": "^15.4.0",
    "prettier": "^1.18.2",
    "rxjs-tslint-rules": "^4.24.0",
    "serve": "^11.0.1",
    "snyk": "^1.175.2",
    "standard-changelog": "^2.0.11",
    "start-server-and-test": "^1.9.1",
    "stylelint": "^10.1.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "ts-loader": "^6.0.2",
    "ts-node": "~8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.4.5",
    "webpack": "4.33.0",
    "webpack-dashboard": "^3.0.7",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
