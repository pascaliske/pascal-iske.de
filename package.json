    "@rstacruz/bump-cli": "^2.0.0",
    "@sentry/cli": "^1.65.0",
    "@types/cors": "^2.8.10",
    "@types/express": "4.17.12",
    "@types/express-serve-static-core": "4.17.20",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "15.6.1",
    "@types/nodemailer": "^6.4.2",
    "@types/webpack": "^5.28.0",
    "bundlewatch": "^0.3.2",
    "chalk": "^4.1.1",
