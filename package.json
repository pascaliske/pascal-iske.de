{
  "name": "pascal-iske",
  "description": "Angular Web App built with Travis CI and deployed with Firebase.",
  "version": "2.1.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "ngcc --tsconfig tsconfig.base.json",
    "ng": "ng",
    "bump": "bump package.json --yes",
    "firebase": "firebase",
    "lighthouse": "lighthouse --quiet --chrome-flags=\"--headless\"",
    "sentry-cli": "sentry-cli",
    "now": "now",
    "bundlewatch": "bundlewatch --config .bundlewatchrc.json",
    "start": "ng serve",
    "build": "run-s build:app build:server build:prerender build:cfn",
    "build:app": "ng build --prod",
    "build:server": "ng run pascal-iske:server:production",
    "build:prerender": "ts-node --project tsconfig.base.json --require esm src/prerender.ts || exit 0",
    "build:cfn": "tsc --project tsconfig.cfn.json && cp src/cfn/package.json dist/cfn/package.json",
    "now-build": "run-s \"build:app --progress false\" \"build:server --progress false\" build:prerender",
    "lint": "ng lint pascal-iske --fix && stylelint src/**/*.scss",
    "e2e": "run-s e2e:verify e2e:prepare",
    "e2e:verify": "cypress verify",
    "e2e:prepare": "start-server-and-test 'yarn e2e:serve' :4200 'yarn e2e:test'",
    "e2e:serve": "serve --config serve.json --listen 4200 --no-clipboard",
    "e2e:test": "cypress run --record false",
    "format": "prettier --write \"**/*.{ts,scss,html,md,json}\"",
    "version": "bump --yes package.json",
    "changelog": "standard-changelog"
  },
  "private": true,
  "prettier": "@pascaliske/prettier-config",
  "engines": {
    "node": ">=10.0.0"
  },
  "resolutions": {
    "@types/chai": "4.2.7",
    "@types/express": "4.17.9",
    "@types/express-serve-static-core": "4.17.14",
    "@types/sinon": "7.5.1"
  },
  "optionalDependencies": {
    "fsevents": "2.2.1"
  },
  "dependencies": {
    "@angular/animations": "^10.0.4",
    "@angular/common": "^10.0.4",
    "@angular/compiler": "^10.0.4",
    "@angular/core": "^10.0.4",
    "@angular/forms": "^10.2.3",
    "@angular/platform-browser": "^10.0.4",
    "@angular/platform-browser-dynamic": "^10.0.4",
    "@angular/platform-server": "^10.0.4",
    "@angular/router": "^10.0.4",
    "@angular/service-worker": "^10.0.4",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@ngx-progressbar/core": "~5.3.2",
    "@ngx-progressbar/http": "~5.3.2",
    "@ngx-progressbar/router": "~5.3.2",
    "@pascaliske/form-elements": "^2.8.1",
    "@pascaliske/html-helpers": "^1.5.8",
    "@pascaliske/ngx-notifications": "^1.5.2",
    "@pascaliske/ngx-sentry": "^2.3.1",
    "@sentry/browser": "^5.28.0",
    "choices.js": "^9.0.1",
    "cookieconsent": "^3.1.1",
    "core-js": "^3.8.0",
    "flatpickr": "^4.6.6",
    "fuzzysearch": "^1.0.3",
    "intersection-observer": "^0.11.0",
    "lodash-es": "^4.17.15",
    "modularscale-sass": "^3.0.10",
    "ngx-cookieconsent": "^2.2.3",
    "ngx-markdown": "^10.1.1",
    "normalize-scss": "^7.0.1",
    "rxjs": "^6.6.3",
    "susy": "^3.0.6",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "^10.0.4",
    "@angular/language-service": "^11.0.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@packtracker/webpack-plugin": "^2.3.0",
    "@pascaliske/ngx-prerenderer": "^2.0.4",
    "@pascaliske/prettier-config": "^1.1.0",
    "@sentry/cli": "^1.60.1",
    "@types/cors": "^2.8.8",
    "@types/express": "4.17.9",
    "@types/express-serve-static-core": "4.17.14",
    "@types/fs-extra": "^9.0.4",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "14.14.10",
    "@types/nodemailer": "^6.4.0",
    "@types/webpack": "^4.41.25",
    "bump-cli": "^1.1.3",
    "bundlewatch": "^0.3.1",
    "chalk": "^4.1.0",
    "codelyzer": "^6.0.1",
    "cors": "^2.8.5",
    "cypress": "^5.6.0",
    "esm": "^3.2.25",
    "firebase-admin": "^9.4.1",
    "firebase-functions": "^3.12.0",
    "firebase-tools": "^8.16.2",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.0",
    "lighthouse": "^6.5.0",
    "lint-staged": "^10.5.2",
    "nodemailer": "^6.4.16",
    "now": "^20.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rxjs-tslint-rules": "^4.34.7",
    "serve": "^11.3.2",
    "standard-changelog": "^2.0.27",
    "start-server-and-test": "^1.11.6",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "ts-loader": "^8.0.11",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.7",
    "webpack": "4.44.2",
    "webpack-dashboard": "^3.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
