    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@pascaliske/form-elements": "^1.1.1",
    "@pascaliske/html-helpers": "^1.2.2",
    "@pascaliske/ngx-notifications": "^1.0.9",
    "@sentry/browser": "^4.1.1",
    "cookieconsent": "^3.1.0",
    "core-js": "^2.5.7",
    "decko": "^1.2.0",
