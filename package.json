    "@types/fs-extra": "^8.0.1",
    "@types/google.analytics": "^0.0.40",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "13.1.6",
    "@types/nodemailer": "^6.4.0",
    "@types/webpack": "^4.41.2",
    "bump-cli": "^1.1.3",
    "bundlewatch": "^0.2.5",
    "chalk": "^3.0.0",
    "codelyzer": "^5.2.1",
    "cors": "^2.8.5",
    "cypress": "^3.8.2",
    "esm": "^3.2.25",
    "firebase-admin": "^8.9.0",
    "firebase-functions": "^3.3.0",
    "firebase-tools": "^7.11.0",
    "fs-extra": "^8.1.0",
    "husky": "^4.0.6",
