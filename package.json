{
  "name": "pascal-iske",
  "description": "Angular Web App built with Travis CI and deployed with Firebase.",
  "version": "1.3.0",
  "license": "MIT",
  "scripts": {
    "ng": "ngw",
    "firebase": "firebase",
    "bundlewatch": "echo TRAVIS_REPO_SLUG && echo CI_REPO_NAME && bundlewatch",
    "start": "ngw serve",
    "build": "ngw build --prod --aot --build-optimizer --stats-json",
    "postbuild": "tsc --project src/tsconfig.cfn.json && cp src/cfn/package.json dist/cfn/package.json",
    "lint": "ngw lint pascal-iske --fix",
    "check": "snyk test && snyk monitor",
    "format": "prettier --config .prettierrc --write src/**/**.{ts,scss,md}"
  },
  "private": true,
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "bundlewatch": {
    "files": [
      {
        "path": "dist/app/*.js",
        "maxSize": "250kB"
      },
      {
        "path": "dist/cfn/*.js",
        "maxSize": "5kB"
      }
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/pwa": "^0.10.3",
    "@angular/router": "^7.0.1",
    "@angular/service-worker": "^7.0.1",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-brands-svg-icons": "^5.4.2",
    "@fortawesome/free-regular-svg-icons": "^5.4.2",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@ngx-progressbar/core": "~5.2.1",
    "@ngx-progressbar/http": "~5.2.1",
    "@ngx-progressbar/router": "~5.2.1",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@pascaliske/form-elements": "^1.1.1",
    "@pascaliske/html-helpers": "^1.2.2",
    "@pascaliske/ngx-notifications": "^1.0.10",
    "@sentry/browser": "4.2.3",
    "cookieconsent": "^3.1.0",
    "core-js": "^2.5.7",
    "decko": "^1.2.0",
    "highlight.js": "^9.13.1",
    "intersection-observer": "^0.5.1",
    "lodash-es": "^4.17.11",
    "modularscale-sass": "^3.0.8",
    "ngx-cookieconsent": "^2.0.0",
    "ngx-markdown": "^7.0.0",
    "normalize-scss": "^7.0.1",
    "rxjs": "^6.3.3",
    "susy": "^3.0.5",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.3",
    "@angular/cli": "^7.0.3",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/language-service": "^7.0.1",
    "@types/cors": "^2.8.4",
    "@types/google.analytics": "^0.0.39",
    "@types/highlight.js": "^9.12.3",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "10.12.0",
    "@types/webpack": "^4.4.17",
    "bundlewatch": "^0.2.1",
    "codelyzer": "^4.5.0",
    "cors": "^2.8.4",
    "firebase-admin": "^6.1.0",
    "firebase-functions": "^2.1.0",
    "firebase-tools": "^6.0.0",
    "ngw": "^7.0.0",
    "nodemailer": "^4.6.8",
    "prettier": "^1.14.3",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "rxjs-tslint-rules": "^4.10.0",
    "snyk": "^1.105.0",
    "ts-node": "~7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "~3.1.3",
    "webpack": "^4.23.1",
    "webpack-dashboard": "^2.0.0",
    "webpack-plugin-replace": "^1.1.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
