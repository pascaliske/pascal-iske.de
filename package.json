{
  "name": "pascal-iske",
  "description": "Angular Web App built with Travis CI and deployed with Firebase.",
  "version": "1.1.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "firebase": "firebase",
    "start": "ngw serve",
    "build": "yarn build-app && yarn build-cfn",
    "build-app": "ngw build --prod --aot --build-optimizer --stats-json",
    "build-cfn": "tsc --project src/tsconfig.cfn.json && cp src/cfn/package.json dist/cfn/package.json",
    "lint": "ngw lint pascal-iske --fix",
    "check": "snyk test",
    "format": "prettier --config .prettierrc --write src/**/**.{ts,scss,md}"
  },
  "private": true,
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "dependencies": {
    "@angular/animations": "^6.1.4",
    "@angular/common": "^6.1.4",
    "@angular/compiler": "^6.1.4",
    "@angular/core": "^6.1.4",
    "@angular/forms": "^6.1.4",
    "@angular/http": "^6.1.4",
    "@angular/platform-browser": "^6.1.4",
    "@angular/platform-browser-dynamic": "^6.1.4",
    "@angular/pwa": "^0.7.5",
    "@angular/router": "^6.1.4",
    "@angular/service-worker": "^6.1.4",
    "@fortawesome/fontawesome-free": "^5.3.0",
    "@ngrx/effects": "^6.1.0",
    "@ngrx/entity": "^6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "^6.1.0",
    "@ngx-progressbar/core": "^5.0.1",
    "@ngx-progressbar/http": "^5.0.1",
    "@ngx-progressbar/router": "^5.0.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@pascaliske/form-elements": "^1.0.3",
    "@pascaliske/html-helpers": "^1.2.1",
    "@pascaliske/ngx-notifications": "^1.0.7",
    "@sentry/browser": "^4.0.0-beta.12",
    "cookieconsent": "^3.1.0",
    "core-js": "^2.5.7",
    "decko": "^1.2.0",
    "highlight.js": "^9.12.0",
    "intersection-observer": "^0.5.0",
    "lodash-es": "^4.17.10",
    "modularscale-sass": "^3.0.5",
    "ngx-cookieconsent": "^2.0.0",
    "ngx-markdown": "^6.2.0",
    "normalize-scss": "^7.0.1",
    "rxjs": "^6.2.2",
    "susy": "^3.0.5",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.5",
    "@angular/cli": "^6.1.5",
    "@angular/compiler-cli": "^6.1.4",
    "@angular/language-service": "^6.1.4",
    "@ngrx/schematics": "^6.1.0",
    "@types/cors": "^2.8.4",
    "@types/google.analytics": "^0.0.39",
    "@types/highlight.js": "^9.12.3",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "~10.9.2",
    "@types/webpack": "^4.4.11",
    "codelyzer": "^4.4.4",
    "cors": "^2.8.4",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.0.5",
    "firebase-tools": "^4.2.0",
    "ngw": "^6.0.8",
    "nodemailer": "^4.6.8",
    "prettier": "^1.14.2",
    "rxjs-tslint-rules": "^4.7.2",
    "snyk": "^1.94.0",
    "ts-node": "~7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "~2.9.2"
  }
}
